pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building master'
				sh javac HelloWorld.java
				sh java HelloWorld
            }
        }
        stage('Test') {
            steps {
                echo 'Testing master'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying master'
            }
        }
    }
}